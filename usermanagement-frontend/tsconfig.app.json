{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
    "esModuleInterop": true,
      "strict": true,
      "jsx": "react-jsx",
      "noEmit": true,
      "skipLibCheck": true,
      "isolatedModules": true,
      "baseUrl": ".",
      "lib": ["ES2020", "DOM", "DOM.Iterable"]
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  